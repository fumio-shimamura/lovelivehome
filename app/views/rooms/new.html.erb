<div class="search">
  <%= form_tag new_room_path , method: :get do %>
    <%= text_field_tag :q , params[:q], placeholder: "キーワードを入力", required: true %>
    <%= f.label :q %>
    <%= file_field_tag :q , params[:q], placeholder: "画像を選択", required: true %>
    <%= submit_tag "画像をアップロード", name: nil , class: "btn btn-success", data: { disable_with: "アップロード中..."} %>
  <% end %>
</div>
